<script>
// Array of popup IDs
const popups = ['popup1', 'popup2', 'popup3', 'popup4'];
let activePopups = [];

// Show random popup
function showRandomPopup() {
  if (activePopups.length < 2) { // Don't show more than 2 at once
    const availablePopups = popups.filter(id => !activePopups.includes(id));
    if (availablePopups.length > 0) {
      const randomPopup = availablePopups[Math.floor(Math.random() * availablePopups.length)];
      document.getElementById(randomPopup).style.display = 'block';
      activePopups.push(randomPopup);
      
      // Make close button randomly move around
      setTimeout(() => makeCloseButtonDifficult(randomPopup), 2000);
    }
  }
}

// Make close button difficult to click
function makeCloseButtonDifficult(popupId) {
  const closeBtn = document.querySelector(`#${popupId} .popup-close`);
  if (closeBtn) {
    closeBtn.addEventListener('mouseenter', function() {
      // Randomly move the button when mouse hovers
      const randomX = Math.random() * 20 - 10;
      const randomY = Math.random() * 20 - 10;
      this.style.transform = `translate(${randomX}px, ${randomY}px) scale(0.7)`;
    });
    
    closeBtn.addEventListener('mouseleave', function() {
      this.style.transform = 'scale(1)';
    });
  }
}

// Close popup function
function closePopup(popupId) {
  document.getElementById(popupId).style.display = 'none';
  activePopups = activePopups.filter(id => id !== popupId);
}

// Start showing popups after page loads
window.addEventListener('load', function() {
  // Show first popup after 3 seconds
  setTimeout(showRandomPopup, 3000);
  
  // Show random popups every 8-15 seconds
  setInterval(() => {
    if (Math.random() > 0.3) { // 70% chance to show popup
      showRandomPopup();
    }
  }, Math.random() * 7000 + 8000);
  
  // Add content shifting to existing elements
  setTimeout(() => {
    const elements = document.querySelectorAll('.moment-box, .conclusion-box, .shocking-reveal');
    elements.forEach((el, index) => {
      setTimeout(() => {
        el.classList.add('expanding-element');
        if (index % 2 === 0) {
          el.classList.add('content-jumper');
        }
      }, index * 1000);
    });
  }, 5000);
  
  // Randomly shift content around
  setInterval(() => {
    const contentElements = document.querySelectorAll('.moment-box, p, h2');
    const randomElement = contentElements[Math.floor(Math.random() * contentElements.length)];
    if (randomElement && Math.random() > 0.8) { // 20% chance
      randomElement.style.transform = `translateX(${Math.random() * 10 - 5}px)`;
      setTimeout(() => {
        randomElement.style.transform = 'translateX(0px)';
      }, 500);
    }
  }, 3000);
});

// Close popup when clicking overlay
document.addEventListener('click', function(e) {
  if (e.target.classList.contains('popup-overlay')) {
    closePopup(e.target.id);
  }
});
</script>